package com.company;

import java.util.Date;

public class Aircraft {
    private final String m_manufacturer;
    private float m_max_speed;
    private float m_max_altitude;
    private final Date m_production_date;

    public Aircraft(String m_manufacturer, float m_max_speed, float m_max_altitude, Date m_production_date) {
        this.m_manufacturer = m_manufacturer;
        this.m_max_speed = m_max_speed;
        this.m_max_altitude = m_max_altitude;
        this.m_production_date = m_production_date;
    }

    public String getM_manufacturer() {
        return this.m_manufacturer;
    }

    public float getM_max_speed() {
        return this.m_max_speed;
    }

    public float getM_max_altitude() {
        return this.m_max_altitude;
    }

    public Date getM_production_date() {
        return this.m_production_date;
    }

    public String toString() {
        return "Aircraft{m_manufacturer='" + this.m_manufacturer + "', m_max_speed=" + this.m_max_speed + ", m_max_altitude=" + this.m_max_altitude + ", m_production_date=" + this.m_production_date + "}";
    }
}
